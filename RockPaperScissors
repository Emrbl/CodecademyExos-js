}
function determineWinner(userChoice, ComputerChoice) {
  if (userChoice === computerChoice) {
  return 'it\'s a tie';
    
  } if (userChoice === 'rock') {    
    if (computerChoice === 'paper') {
    return 'computer wins';
    } else {
      return 'you win!';
    }
  } if (userChoice === 'paper') {
    if (computerChoice === 'scissors') {
      return 'computer wins';
    } else {
      return 'you win!'
    }
  }
  }
